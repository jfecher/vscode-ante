{
    "name": "ante",
    "displayName": "Ante",
    "description": "Ante language support for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "jfecher",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ante",
            "aliases": ["Ante", "ante"],
            "extensions": [".an"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ante",
            "scopeName": "source.ante",
            "path": "./syntaxes/ante.tmLanguage.json"
        }]
    }
}
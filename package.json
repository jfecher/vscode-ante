{
    "name": "ante",
    "displayName": "Ante",
    "description": "Ante language support for Visual Studio Code",
    "version": "0.1.0",
    "publisher": "jfecher",
    "repository": {
        "type": "git",
        "url": "https://github.com/jfecher/vscode-ante.git"
    },
    "keywords": [
        "language",
        "ante",
        "syntax",
        "highlighting"
    ],
    "homepage": "https://github.com/jfecher/vscode-ante",
    "galleryBanner": {
        "color": "#da4d4d",
        "theme": "dark"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ante",
                "aliases": [
                    "Ante",
                    "ante"
                ],
                "extensions": [
                    ".an"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ante",
                "scopeName": "source.ante",
                "path": "./syntaxes/ante.tmLanguage.json"
            }
        ]
    }
}
